   <SolidColorBrush x:Key="WindowBorderColor" Color="#6C929F" PresentationOptions:Freeze="True"/>
   <SolidColorBrush x:Key="TextSelectionColor" Color="#2080A0" PresentationOptions:Freeze="True" />
   <SolidColorBrush x:Key="TextColor" Color="#DEF" PresentationOptions:Freeze="True"/>
   <SolidColorBrush x:Key="AcceptCommitBackgroundColor" Color="#C281" PresentationOptions:Freeze="True"/>
   <SolidColorBrush x:Key="AcceptCommitGlyphBackgroundColor" Color="#84F2" PresentationOptions:Freeze="True"/>
   <SolidColorBrush x:Key="AbortCommitBackgroundColor" Color="#C812" PresentationOptions:Freeze="True"/>
   <SolidColorBrush x:Key="AbortCommitGlyphBackgroundColor" Color="#8F24" PresentationOptions:Freeze="True"/>
